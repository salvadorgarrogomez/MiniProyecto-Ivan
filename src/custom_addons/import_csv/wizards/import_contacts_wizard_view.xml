<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_import_contacts_wizard_form" model="ir.ui.view">
        <field name="name">import.contacts.wizard.form</field>
        <field name="model">import.contacts.wizard</field>
        <field name="arch" type="xml">
            <form string="Import Contacts">
                <group>
                    <field name="file_data" filename="file_name"/>
                </group>
                <footer>
                    <button string="Import" type="object" name="import_contacts" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>

